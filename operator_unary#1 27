#include <iostream>
using namespace std;

int main(){
    //operator unary
    int a = 10;
    a = a++; //post - increment
    
    
    int b = 10;
    b = ++b; //pre - increment
    
    
    //decrement
    int c = 10;
    c = c--; //post - decrement
    
    int d = 10;
    d = --d; //pre - decrement
    
    cout<<a<<endl;
    cout<<b<<endl;
    cout<<c<<endl;
    cout<<d<<endl;
    
    
    
    return 0;
}
